{
  "version": 2,
  "builds": [
    {
      "src": "main.serverless.ts",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb",
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    { "src": "/", "dest": "main.serverless.ts" },
    { "src": "/(.*)", "dest": "main.serverless.ts" }
  ]
}